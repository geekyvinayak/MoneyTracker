{
  "version": 2,

  "builds": [
    {
      "src": "index.js",

      "use": "@vercel/node",

      "config": {}
    }
  ],

  "routes": [
    {
      "src": "/(.*)",

      "dest": "index.js"
    }
  ],
   "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "https://moneytracker-sigma.vercel.app//(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }  
    ]
}
